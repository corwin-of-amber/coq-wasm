{
    "rootdir": "vendor/coq",
    "builddir": "coq-pkgs",
    "bundle": "coq",
    "projects": {
        "init": {
            "theories": {
                "prefix": "Coq",
                "dirpaths": ["Init", "Bool", "Unicode",
                             "btauto", "ssr", "ssrmatching"]
            },
            "plugins": {
                "prefix": "Coq",
                "dirpaths": ["ltac", "syntax", "btauto", "cc", "firstorder",
                             "ssr", "ssrmatching"]
            }
        },
        "coq-base": {
            "theories": {
                "prefix": "Coq",
                "dirpaths": ["Logic", "Program", "Classes", "Compat", "Structures",
                             "Relations", "Setoids", 
                             "extraction", "funind", "derive", "ssrsearch", "rtauto"]
            },
            "plugins": {
                "prefix": "Coq",
                "dirpaths": ["extraction", "funind", "derive", "ssrsearch", "rtauto"]
            }
        },
        "coq-collections": {
            "theories": {
                "prefix": "Coq",
                "dirpaths": ["Lists", "Vectors", "Sets", "FSets", "MSets",
                             "Sorting", "Array", "Strings"]
            }
        },
        "coq-arith": {
            "theories": {
                "prefix": "Coq",
                "dirpaths": ["Arith", "NArith", "PArith", "ZArith", "QArith",
                             "Numbers", "Wellfounded", "setoid_ring", "omega",
                             "micromega"]
            },
            "plugins": {
                "prefix": "Coq",
                "dirpaths": ["ring", "micromega"]
            }
        },
        "coq-reals": {
            "theories": {
                "prefix": "Coq",
                "dirpaths": ["Reals", "Floats", "nsatz"]
            },
            "plugins": {
                "prefix": "Coq",
                "dirpaths": ["nsatz"]
            }
        },
        "ltac2": {
            "user-contrib/Ltac2": {
                "prefix": "Ltac2"
            }
        }
    }
}
